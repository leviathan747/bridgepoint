<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
  <extension
       id="org.xtuml.bp.mc.mc3020.MCNature"
       name="model compiler nature"
       point="org.eclipse.core.resources.natures">
    <runtime>
      <run class="org.xtuml.bp.mc.mc3020.MCNature"></run>
    </runtime>
    <builder id="org.xtuml.bp.mc.mc3020.c_builder"></builder>
  </extension>
  <extension point="org.eclipse.ui.ide.projectNatureImages">
    <image icon="platform:/plugin/org.xtuml.bp.core/icons/xtuml-nature.gif"
           natureId="org.xtuml.bp.mc.mc3020.MCNature"
           id="org.xtuml.resoures.natureimage">
    </image>
  </extension>
  <extension
        id="org.xtuml.bp.mc.mc3020.c_builder"
        name="C Model Compiler (Source)"
        point="org.eclipse.core.resources.builders">
    <builder>
      <run class="org.xtuml.bp.mc.mc3020.Mc3020CSourceBuilder" />
    </builder>
  </extension>
  <extension point="org.xtuml.bp.core.model-compilers">
    <model-compiler name="C Model Compiler (Source)" class="org.xtuml.bp.mc.mc3020.MCNewProjectWizard"></model-compiler>
  </extension>
  <extension point="org.eclipse.ui.startup">
    <startup class="org.xtuml.bp.mc.mc3020.Activator"/>
  </extension>
</plugin>
